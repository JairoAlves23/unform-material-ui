{
  "name": "unform-material-ui",
  "version": "0.1.0",
  "author": "Italo Izaac <italoiz.dev@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "lib/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "__tests__/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "example/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:start": "rollup -c -w",
    "dev:example": "cd example && npm run start",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "@material-ui/core": "^4.0.2",
    "@rocketseat/unform": "^1.4.0",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/yup": "^0.26.14",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "commitlint-config-rocketseat": "^0.0.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-url": "^2.2.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1",
    "yup": "^0.27.0"
  },
  "peerDependencies": {
    "@material-ui/core": ">= 4.0.2",
    "@rocketseat/unform": ">= 1.4.0",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  }
}
